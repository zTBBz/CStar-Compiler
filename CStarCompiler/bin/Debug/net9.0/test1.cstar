module GameLogic;

use Math;
@CLibrary use OpenGl;

public struct Player : Create
{
    int health;
    int x;
    int y;

    public Player Create()
    {
        var instance = Player;
        instance.health = 100;
        instance.x = 0;
        instance.y = 0;
        return instance;
    }
}

public void Update(Player p)
{
    var temp = 10;
    p.x = p.x + temp;
    
    if (p.x > 100)
    {
        Log(p.x);
        return;
    }
}
